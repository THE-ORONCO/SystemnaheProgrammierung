                       178     START_TIMER_DOOR_1:
008A D28C              179         SETB tr0; start timer0
                       180
008C 7905              181         MOV r1, #5h ; r1 auf 5 setzen; im echten Leben sollte das 39368
008E 020067            182         LJMP CONTINUE_AFTER_TIMER_1_SET
                       183
                       184
                       185     ; sobald Tür 1 geöffnet ist, läuft ein Timer, der bei jedem Interupt hierher springt
                       186     TIMEOUT_TIMER_DOOR_1:
                       187         ; INC       r1 ; Hochzählen, wie oft der Timer abgelaufen ist
                       188         ; CJNE      r1, #5h, TMP ; Schleife der Wiederholungen des Timers (wir brauchen 39368 Wiederholun
                       189
0091 D90B              190         DJNZ r1, TMP; r1 dekrementieren und wegspringen, wenn ungleich 0 (Decrement Jump Not Zero)
                       191
                       192         ; Hier gehts weiter, wenn der Timer oft genug abgelaufen ist (wenn die Tür lange genug offen war)
                       193         ; -> Timer & Motor ausschalten und Stop-Anfragen resetten
                       194         ; MOV       r1, #00h
0093 C28C              195         CLR tr0 ; stop timer0
                       196
0095 C218              197         CLR STOP_1_FF; Resetten des Stop 1 FlipFlop
0097 C21A              198         CLR OPEN_1_FF ; Resetten des Open 1 FlopFlop
0099 C210              199         CLR OPEN_1 ; Motor nicht mehr auf öffnend setzen
009B D212              200         SETB CLOSE_1 ; Motor auf schließend setzen
                       201
009D 22                202         RET
                       203
                       204     TMP:
009E 22                205         RET
                       206
                       207     SET_STOP_1_FF:
