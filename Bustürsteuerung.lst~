                        14     ; Eingabevektoren TAST und SENS
                        15     ; Taster:
                        16     ; STOP_IN_1     Taster, zum Öffnen der Tür 1 von innnen (Stop-Taster)
                        17     ; STOP_IN_2     Taster, zum Öffnen der Tür 2 von innnen (Stop-Taster)
                        18     ; STOP_OUT_1    Taster, zum Öffnen der Tür 1 von außen
                        19     ; STOP_OUT_2    Taster, zum Öffnen der Tür 2 von außen
                        20     ; DRIVERS_OK    Freigabe-Taster des Busfahrers
                        21     ;
                        22     ; Sensoren:
                        23     ; OPENED_1      Ausgabe des Geöffnet-Sensors der Tür 1
                        24     ; OPENED_2      Ausgabe des Geöffnet-Sensors der Tür 2
                        25     ; CLOSED_1      Ausgabe des Geschlossen-Sensors der Tür 1
                        26     ; CLOSED_2      Ausgabe des Geschlossen-Sensors der Tür 2
                        27     ; BLOCKED_1     Jemand steht in der schließenden Tür 1
                        28     ; BLOCKED_2     Jemand steht in der schließenden Tür 2
                        29
                        30
                        31
                        32
                        33     ; Zeug das Menschen verändern können (Taster)
  0020                  34     TAST EQU 20H
  0000                  35     STOP_IN_1  EQU TAST.0
  0001                  36     STOP_IN_2  EQU TAST.1
  0002                  37     STOP_OUT_1 EQU TAST.2
  0003                  38     STOP_OUT_2 EQU TAST.3
  0004                  39     DRIVERS_OK EQU TAST.4
                        40
                        41     ; Das was der Bus fühlt (Gefühle bzw. Sensoren)
  0021                  42     SENS EQU 21H
  0008                  43     OPENED_1   EQU SENS.0
  0009                  44     OPENED_2   EQU SENS.1
  000A                  45     CLOSED_1   EQU SENS.2
  000B                  46     CLOSED_2   EQU SENS.3
  000C                  47     BlOCKED_1  EQU SENS.4
  000D                  48     BlOCKED_2  EQU SENS.5
                        49
                        50     ; Ausgabevektor
                        51     ; OPEN_1        Tür 1 soll geöffnet werden
                        52     ; OPEN_2        Tür 2 soll geöffnet werden
                        53     ; CLOSE_1       Tür 1 soll geschlossen werden
                        54     ; CLOSE_2       Tür 2 soll geschlossen werden
                        55
  0022                  56     MOTR EQU 22H
  0010                  57     OPEN_1 EQU MOTR.0
  0011                  58     OPEN_2 EQU MOTR.1
  0012                  59     CLOSE_1 EQU MOTR.2
  0013                  60     CLOSE_2 EQU MOTR.3
                        61
                        62     ; FlipFlops zum Zwischenspeichern
                        63
  0023                  64     FliFlo EQU 23H
  0018                  65     STOP_1_FF EQU FliFlo.0
  0019                  66     STOP_2_FF EQU FliFlo.1
  001A                  67     OPEN_1_FF EQU FliFlo.2
  001B                  68     OPEN_2_FF EQU FliFlo.3
