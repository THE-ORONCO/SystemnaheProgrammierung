                        19     ; Eingabevektoren TAST und SENS
                        20     ; Taster:
                        21     ; STOP_IN_1     Taster, zum Öffnen der Tür 1 von innnen (Stop-Taster)
                        22     ; STOP_IN_2     Taster, zum Öffnen der Tür 2 von innnen (Stop-Taster)
                        23     ; STOP_OUT_1    Taster, zum Öffnen der Tür 1 von außen
                        24     ; STOP_OUT_2    Taster, zum Öffnen der Tür 2 von außen
                        25     ; DRIVERS_OK    Freigabe-Taster des Busfahrers
                        26     ;
                        27     ; Sensoren:
                        28     ; OPENED_1      Ausgabe des Geöffnet-Sensors der Tür 1
                        29     ; OPENED_2      Ausgabe des Geöffnet-Sensors der Tür 2
                        30     ; CLOSED_1      Ausgabe des Geschlossen-Sensors der Tür 1
                        31     ; CLOSED_2      Ausgabe des Geschlossen-Sensors der Tür 2
                        32     ; BLOCKED_1     Jemand steht in der schließenden Tür 1
                        33     ; BLOCKED_2     Jemand steht in der schließenden Tür 2
                        34
                        35
                        36
                        37
                        38     ; Zeug das Menschen verändern können (Taster)
  0020                  39     TAST EQU 20H
  0000                  40     STOP_IN_1  EQU TAST.0
  0001                  41     STOP_IN_2  EQU TAST.1
  0002                  42     STOP_OUT_1 EQU TAST.2
  0003                  43     STOP_OUT_2 EQU TAST.3
  0004                  44     DRIVERS_OK EQU TAST.4
                        45
                        46     ; Das was der Bus fühlt (Gefühle bzw. Sensoren)
  0021                  47     SENS EQU 21H
  0008                  48     OPENED_1   EQU SENS.0
  0009                  49     OPENED_2   EQU SENS.1
  000A                  50     CLOSED_1   EQU SENS.2
  000B                  51     CLOSED_2   EQU SENS.3
  000C                  52     BlOCKED_1  EQU SENS.4
  000D                  53     BlOCKED_2  EQU SENS.5
                        54
                        55     ; Ausgabevektor
                        56     ; OPEN_1        Tür 1 soll geöffnet werden
                        57     ; OPEN_2        Tür 2 soll geöffnet werden
                        58     ; CLOSE_1       Tür 1 soll geschlossen werden
                        59     ; CLOSE_2       Tür 2 soll geschlossen werden
                        60
  0022                  61     MOTR EQU 22H
  0010                  62     OPEN_1 EQU MOTR.0
  0011                  63     OPEN_2 EQU MOTR.1
  0012                  64     CLOSE_1 EQU MOTR.2
  0013                  65     CLOSE_2 EQU MOTR.3
                        66
                        67     ; FlipFlops zum Zwischenspeichern
                        68
  0023                  69     FF EQU 23H
  0018                  70     STOP_1_FF EQU FF.0
  0019                  71     STOP_2_FF EQU FF.1
  001A                  72     OPEN_1_FF EQU FF.2
  001B                  73     OPEN_2_FF EQU FF.3
                        74     ; INITIALISIERUNG
                        75     Init:
001F 752000             76         MOV TAST, #00H
0022 752100             77         MOV SENS, #00H ; oxC um die Closed Variablen zu setzen, wird duch P1 gesetzt
                        78         ; MOV CLOSED_1, 1 ; Tür eins ist geschlossen
0025 752200             79         MOV MOTR, #00H
                        80
0028 758000             81         MOV P0, #00H ; P0 wird verwendet um TAST von der IDE anzusprechen
002B 75900C             82         MOV P1, #0CH ; P1 wird verwendet um SENS in der IDE anzuzeigen
002E 75A000             83         MOV P2, #00H ; P2 wird verwendet um MOTR in der IDE anzuzeigen
                        84
0031 752300             85         MOV FF, #00H
                        86
                        87         ; TIMER Kram
0034 75A89A             88         MOV IE, #10011010b
0037 758922             89         MOV tmod, #00100010b ; 1. Bit für Timer0 -> mod 2 und 5. Bit für Timer1 -> mod 2
003A 758A00             90         MOV tl0, #000h  ; Timer0-Initionalsierung
003D 758C01             91         MOV th0, #001h
                        92
0040 758B00             93         MOV tl1, #000h  ; Timer1-Initionalsierung
0043 758D01             94         MOV th1, #001h
                        95
0046 020049             96         LJMP Anfang
