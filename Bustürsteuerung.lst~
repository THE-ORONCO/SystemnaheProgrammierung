                        93     ;-----------------------------------------
                        94
                        95     Anfang:
                        96         ; Eingaben aus Port 0 in TAST schreiben und SENS und MOTR in Port 1 und 2 (IDE-Anzeige) schreiben
                        97         MOV TAST, P0
                        98         MOV SENS, P1
                        99         MOV P2, MOTR
                       100         MOV P3, FF
                       101
                       102         ; Abfrage ob ein Stop-Taster (innen oder außen) für Tür 1 gedrückt wurder
                       103         MOV C, STOP_IN_1
                       104         ORL C, STOP_OUT_1
                       105         JC SET_STOP_1_FF
                       106
                       107     CONTINUE_AFTER_STOP_1_FF_SET:
                       108
                       109         ; Wenn Stop-Taster 1 (FlipFlop) und Freigabe gesetzt, OPEN_1 (Motor 1) auf 1 setzen
                       110         MOV C, STOP_1_FF
                       111         ANL C, DRIVERS_OK
                       112         JC SET_OPEN_1_FF
                       113
                       114     CONTINUE_AFTER_OPEN_1_FF_SET:
                       115
                       116
                       117         ; Platz für die 2. Tür
                       118         ; ...
                       119
                       120
                       121         ; Schauen ob die Tür 1 geöffnet ist (Endtaster gesetzt) und wenn ja den Motor beenden und Timer s
                       122         MOV C, OPEN_1
                       123         ANL C, OPENED_1
                       124         JC START_TIMER_DOOR_1
                       125     CONTINUE_AFTER_TIMER_1_SET:
                       126
                       127
                       128         ; Platz für die 2. Tür
                       129         ; ...
                       130
                       131
                       132         ; Wenn Tür nicht zu ist und der Zu-Motor an ist, dann schaue ob Blockiert
                       133         MOV C, CLOSED_1
                       134         CMP C
****ERROR:Unknownkeyword:`CMP'`CMP'isneithermacronorinstructionnordirective
                       135         ANL C, CLOSE_1
                       136         JC CHECK_FOR_BLOCKED_DOOR_1
                       137
                       138     CONTINUE_AFTER_CHECK_FOR_BLOCKED_DOOR_1:
                       139
                       140         ; ...
                       141
                       142         LJMP Anfang
                       143
                       144
                       145     ;-------------------------------------------------
                       146     ; Einzele Programmschnippsel
                       147     ;-------------------------------------------------
                       148
                       149
                       150     START_TIMER_DOOR_1:
                       151         SETB tr0; start timer0
                       152         LJMP CONTINUE_AFTER_TIMER_1_SET
                       153
                       154
                       155     ; sobald Tür 1 geöffnet ist, läuft ein Timer, der bei jedem Interupt hierher springt
                       156     TIMEOUT_TIMER_DOOR_1:
                       157         INC r1 ; Hochzählen, wie oft der Timer abgelaufen ist
                       158         CJNE        r1, #5h, TMP ; Schleife der Wiederholungen des Timers (wir brauchen 39368 Wiederholun
                       159
                       160         ; Hier gehts weiter, wenn der Timer oft genug abgelaufen ist (wenn die Tür lange genug offen war)
                       161         ; -> Timer & Motor ausschalten und Stop-Anfragen resetten
                       162         MOV r1, #00h
                       163         CLR tr0 ; stop timer0
                       164
                       165         CLR STOP_1_FF; Resetten des Stop 1 FlipFlop
                       166         CLR OPEN_1_FF ; Resetten des Open 1 FlopFlop
                       167         CLR OPEN_1 ; Motor nicht mehr auf öffnend setzen
                       168         SETB CLOSE_1 ; Motor auf schließend setzen
                       169
                       170         RET
                       171
                       172     TMP:
                       173         RET
                       174
                       175     SET_STOP_1_FF:
                       176         SETB STOP_1_FF
                       177         LJMP CONTINUE_AFTER_STOP_1_FF_SET
                       178
                       179     SET_OPEN_1_FF:
                       180         SETB OPEN_1_FF
                       181         SETB OPEN_1
                       182         CLR CLOSE_1
                       183         LJMP CONTINUE_AFTER_OPEN_1_FF_SET
                       184
                       185     CHECK_FOR_BLOCKED_DOOR_1:
                       186         MOV C, BlOCKED_1
                       187         JC SET_OPEN_1_FF
                       188         LJMP CONTINUE_AFTER_CHECK_FOR_BLOCKED_DOOR_1
                       189
