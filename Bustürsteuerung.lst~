                       103     Anfang:
                       104         ; Eingaben aus Port 0 in TAST schreiben und SENS und MOTR in Port 1 und 2 (IDE-Anzeige) schreiben
0049 858020            105         MOV TAST, P0
004C 859021            106         MOV SENS, P1
004F 8522A0            107         MOV P2, MOTR
0052 8523B0            108         MOV P3, FF
                       109
                       110         ; ------------------------------------------ Tür 1
                       111
                       112         ; Abfrage ob ein Stop-Taster (innen oder außen) für Tür 1 gedrückt wurder
0055 A200              113         MOV C, STOP_IN_1
0057 7202              114         ORL C, STOP_OUT_1
0059 4044              115         JC SET_STOP_1_FF
                       116
                       117     CONTINUE_AFTER_STOP_1_FF_SET:
                       118
                       119         ; Wenn Stop-Taster 1 (FlipFlop) und Freigabe gesetzt, OPEN_1 (Motor 1) auf 1 setzen
005B A218              120         MOV C, STOP_1_FF
005D 8204              121         ANL C, DRIVERS_OK
005F 4043              122         JC SET_OPEN_1_FF
                       123
                       124     CONTINUE_AFTER_OPEN_1_FF_SET:
                       125
                       126         ; Schauen ob die Tür 1 geöffnet ist (Endtaster gesetzt) und wenn ja den Motor beenden und Timer s
0061 A210              127         MOV C, OPEN_1
0063 8208              128         ANL C, OPENED_1
0065 4023              129         JC START_TIMER_DOOR_1
                       130     CONTINUE_AFTER_TIMER_1_SET:
                       131
                       132         ; Wenn Tür nicht zu ist und der Zu-Motor an ist, dann schaue ob Blockiert
0067 A20A              133         MOV C, CLOSED_1
0069 B3                134         CPL C
006A 8212              135         ANL C, CLOSE_1
006C 403F              136         JC CHECK_FOR_BLOCKED_DOOR_1
                       137
                       138     CONTINUE_AFTER_CHECK_FOR_BLOCKED_DOOR_1:
                       139
                       140         ; ------------------------------------------ Tür 2
                       141
                       142         ; Abfrage ob ein Stop-Taster (innen oder außen) für Tür 2 gedrückt wurder
006E A201              143         MOV C, STOP_IN_2
0070 7203              144         ORL C, STOP_OUT_2
0072 4054              145         JC SET_STOP_2_FF
                       146
                       147     CONTINUE_AFTER_STOP_2_FF_SET:
                       148
                       149         ; Wenn Stop-Taster 2 (FlipFlop) und Freigabe gesetzt, OPEN_2 (Motor 2) auf 1 setzen
0074 A219              150         MOV C, STOP_2_FF
0076 8204              151         ANL C, DRIVERS_OK
0078 4053              152         JC SET_OPEN_2_FF
                       153
                       154     CONTINUE_AFTER_OPEN_2_FF_SET:
                       155
